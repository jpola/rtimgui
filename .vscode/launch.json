    "version": "0.2.0",
    "configurations": [
        {
            "name": "AOTrace Debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/HIPRT-AO-d.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Debug/",
            "environment": [],
            "console": "integratedTerminal"
        },
        {
            "name": "AOTrace Debug AMDLOG",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/HIPRT-AO-d.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/Debug/",
            "environment": [
                {
                    "name": "AMD_LOG_LEVEL",
                    "value": "7"
                },
                {
                    "name": "AMD_LOG_LEVEL_FILE",
                    "value": "amd.log"
                }
            ],
            "console": "integratedTerminal"
        }
    ]
}